<Window x:Class="imageCorruptor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:imageCorruptor"
        mc:Ignorable="d"
        Title="Image Corruptor" Height="157.78" Width="381.771">
    <Grid>
        <Label x:Name="filepath" Content="" Margin="10,10,85,0" VerticalAlignment="Top"/>
        <Button x:Name="loadimg" Content="Load Image" Margin="0,10,10,0" VerticalAlignment="Top" Height="26" Click="Loadimg_click" HorizontalAlignment="Right" Width="75"/>
        <Button x:Name="done" Content="Done" Margin="269,0,10,10" Height="20" VerticalAlignment="Bottom" Click="Done_Click"/>
        <TextBox x:Name="Tries" Height="23" Margin="65,41,169,0" TextWrapping="Wrap" VerticalAlignment="Top" TextChanged="Tries_TextChanged" Text="15"/>
        <TextBox x:Name="Level" Height="23" Margin="65,69,169,0" TextWrapping="Wrap" VerticalAlignment="Top" TextChanged="Level_TextChanged" Text="2"/>
        <Label Content="Tries" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Height="23" Width="40"/>
        <Label Content="Strength" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,69,0,0" Height="28"/>

    </Grid>
</Window>
